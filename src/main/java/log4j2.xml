<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
    <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} -> %msg%n" />
    <!--
      <PatternLayout>
        <MarkerPatternSelector defaultPattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} -> %msg%n">
          <PatternMatch key="TESTING" pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} -> %msg"/>
          <PatternMatch key="SUCCESS" pattern="%msg%n"/>
          <PatternMatch key="FAILURE" pattern="%msg%n"/>
        </MarkerPatternSelector>
      </PatternLayout> 
        -->
    </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>